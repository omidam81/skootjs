h1.hiddentitle SkootJobs | Find a Job in Malaysia
#toast-container.ng-scope.toast-top-right(ng-class='[config.position, config.animation]', toaster-options="{'time-out': 3000}")
  // ngRepeat: toaster in toasters
#toast-container.ng-scope.toast-center(ng-class='[config.position, config.animation]', toaster-options="{'time-out': 0, 'position-class': 'toast-center', 'toaster-id': 2}")
  // ngRepeat: toaster in toasters
.body-wrap.relative
  scoot-loading.ng-isolate-scope
    #loader.spinner-wrapper(style='display: none;')
      .skoot-spinner-v2(ng-hide='timeOut')
        .spinner
          .cube1
          .cube2
      // ngIf: isCandidate()
      // ngIf: isCompany()
      .text-center.timeout-msg.ng-hide(ng-show='timeOut')
        img.logo-grayscale(src='https://www.skootjobs.com/images/logo.png')
        p
          | We're sorry, things don't appear to be working at the moment. Please try refreshing the page.
  // uiView:
  .ng-scope(ui-view='')
    .about-us.ng-scope
      .cover
        // ngIf: !isMobileRes()
        img.ng-scope(ng-if='!isMobileRes()', src='https://www.skootjobs.com/images/covers/skoot-about-cover.jpg')
        // end ngIf: !isMobileRes()
        .cover-overlay
        .container.cover-bottom
          .row
            .col-xs-12.col-sm-12.col-md-2.hidden-xs.hidden-sm
            .col-xs-12.col-sm-12.col-md-3
              p.sitename SkootJobs.com
              p.desc
                i.fa.fa-bookmark(aria-hidden='true')
                |  Online job platform
            .col-xs-12.col-sm-12.col-md-3
              p.desc.single
                i.fa.fa-map-marker(aria-hidden='true')
                |  Selangor, Malaysia
            .col-xs-12.col-sm-12.col-md-4
              ul.social-icons
                li#social-icons-more
                  a(href='#') +2 More
                  .tooltip-icons
                    ul
                      // ngRepeat: item in socialMenu | limitTo:-2
                      li.ng-scope(ng-repeat='item in socialMenu | limitTo:-2')
                        a.icon(href='https://www.instagram.com/skootjobs/')
                          i.fa.fa-instagram(aria-hidden='true')
                      // end ngRepeat: item in socialMenu | limitTo:-2
                      li.ng-scope(ng-repeat='item in socialMenu | limitTo:-2')
                        a.icon(href='https://plus.google.com/116394090112892290475/about')
                          i.fa.fa-google-plus(aria-hidden='true')
                      // end ngRepeat: item in socialMenu | limitTo:-2
                // ngRepeat: item in socialMenu | limitTo:3
                li.ng-scope(ng-repeat='item in socialMenu | limitTo:3')
                  a.icon(href='https://www.linkedin.com/company/skoot-jobs')
                    i.fa.fa-linkedin(aria-hidden='true')
                // end ngRepeat: item in socialMenu | limitTo:3
                li.ng-scope(ng-repeat='item in socialMenu | limitTo:3')
                  a.icon(href='https://www.facebook.com/skootjobs/')
                    i.fa.fa-facebook(aria-hidden='true')
                // end ngRepeat: item in socialMenu | limitTo:3
                li.ng-scope(ng-repeat='item in socialMenu | limitTo:3')
                  a.icon(href='https://twitter.com/SkootJobs')
                    i.fa.fa-twitter(aria-hidden='true')
                // end ngRepeat: item in socialMenu | limitTo:3
